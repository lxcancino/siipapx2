	<queryString><![CDATA[SELECT (SELECT S.clave FROM sucursal S WHERE V.SUCURSAL_ID=S.ID) AS SUC,D.DOCUMENTO AS RMD,D.FECHA,D.PARCIAL
,F.TIPO,F.FECHA AS FECHA_FAC,F.DOCUMENTO,C.CLAVE,C.NOMBRE,D.IMPORTE,D.IMPUESTO,D.TOTAL,F.TOTAL AS VENTATOTAL
,IF (F.TOTAL=D.TOTAL, "TOT." , "PARC." ) AS TIPO_RMD,U.NOMBRE AS FACTURO
FROM devolucion_de_venta D  
JOIN cobro N ON(N.ID=D.COBRO_ID)
JOIN venta V ON(V.ID=D.VENTA_ID)
JOIN cuenta_por_cobrar F ON(V.cuenta_por_cobrar_id=F.ID)
JOIN cliente C on(F.CLIENTE_ID=C.ID)
JOIN user U ON (U.username=V.create_user)
where D.FECHA BETWEEN $P{FECHA_INI} AND $P{FECHA_FIN} AND V.SUCURSAL_ID LIKE $P{SUCURSAL} AND F.TIPO LIKE $P{ORIGEN}]]></queryString>

